{
  "name": "@iconsans/react",
  "version": "1.0.6-alpha.0",
  "description": "+320 Free Icons for your next project, featuring a bold and linear style that will make your designs stand out. ",
  "keywords": [
    "iconsans",
    "icons",
    "pack",
    "react",
    "svg"
  ],
  "author": {
    "name": "Morteza Sabihi",
    "email": "morteza.sabihi@gmail.com",
    "url": "https://mortezasabihi.com"
  },
  "license": "MIT",
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    "./linear": {
      "import": "./dist/linear.js",
      "require": "./dist/linear.cjs"
    },
    "./bold": {
      "import": "./dist/bold.js",
      "require": "./dist/bold.cjs"
    }
  },
  "files": [
    "dist/*",
    "bold.d.ts",
    "linear.d.ts",
    "index.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "ignore": [
      "!dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mortezasabihi/iconsans.git"
  },
  "scripts": {
    "preconvert": "rimraf src/components",
    "convert": "npx @svgr/cli --config-file ./svgr.config.json -- ../../icons",
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && vite build"
  },
  "bugs": {
    "url": "https://github.com/mortezasabihi/iconsans/issues"
  },
  "homepage": "https://mortezasabihi.github.io/iconsans",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@svgr/cli": "^7.0.0",
    "@types/react": "^18.2.14",
    "@vitejs/plugin-react": "^4.0.1"
  }
}
